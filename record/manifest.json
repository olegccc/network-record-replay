{
   "author": "Oleg Gordeev",
   "background": {
      "persistent": true,
      "scripts": [ "index.js" ]
   },
   "browser_action": {
      "name": "Click to record network history"
   },
   "description": "Records all network history from Chrome tab to JSON file just in two clicks",
   "key": "",
   "manifest_version": 2,
   "name": "Network Record",
   "permissions": [ "webRequest", "<all_urls>", "debugger", "downloads", "notifications", "proxy" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.1"
}
