{
  "app": {
    "background": {
      "scripts": [
        "main.js"
      ]
    }
  },
  "author": "Oleg Gordeev",
  "description": "Replays all network history saved by Network Record extension",
  "key": "",
  "manifest_version": 2,
  "name": "Network Replay",
  "permissions": [
    "system.network",
    "fileSystem",
    "storage"
  ],
  "sockets": {
    "tcpServer": {
      "listen": [
        "*"
      ]
    }
  },
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.10"
}
